    "@opentelemetry/sdk-node": "^0.200.0",
    "express": "^5.1.0",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.6.1",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "pg": "^8.11.3",
    "mysql2": "^3.9.1",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^10.0.0",
    "biome": "^0.3.3",
    "fastify": "^5.3.2",
    "husky": "^8.0.3",
    "typescript": "^5.1.3"
  }
